{
    "name": "altmm",
    "version": "1.0.0",
    "bin": "dist/index.js",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "curl": "curl https://raw.githubusercontent.com/metamath/set.mm/develop/set.mm -o set.mm",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "set.mm": "ts-node ./src/index.ts set.mm set-mm-output.txt",
        "start": "ts-node ./src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Antony74/alt-mm.git"
    },
    "author": "Antony74",
    "license": "CC0-1.0",
    "bugs": {
        "url": "https://github.com/Antony74/alt-mm/issues"
    },
    "homepage": "https://github.com/Antony74/alt-mm#readme",
    "dependencies": {
        "checkmm": "^1.1.0"
    },
    "devDependencies": {
        "@types/node": "^18.7.13",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "eslint": "^8.22.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "prettier": {
        "semi": true,
